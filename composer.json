{
    "name": "arodu/cakephp-admin-tools",
    "description": "AdminTools plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "AdminTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminTools\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
